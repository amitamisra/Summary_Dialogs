aux(decide-19, may-15)
advmod(decide-19, at-16)
det(point-18, some-17)
pobj(at-16, point-18)
ccomp(made-7, decide-19)
aux(overturn-21, to-20)
xcomp(decide-19, overturn-21)
det(decision-23, the-22)
dobj(overturn-21, decision-23)
aux(allow-25, to-24)
vmod(decision-23, allow-25)
