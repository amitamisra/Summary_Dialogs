   straight people that are n't married ca n't get it 
root(ROOT-0, get-9)
amod(people-2, straight-1)
nsubj(get-9, people-2)
nsubjpass(married-6, that-3)
auxpass(married-6, are-4)
neg(married-6, n't-5)
rcmod(people-2, married-6)
aux(get-9, ca-7)
neg(get-9, n't-8)
dobj(get-9, it-10)

