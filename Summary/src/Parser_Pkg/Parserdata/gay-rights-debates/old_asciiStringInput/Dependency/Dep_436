amod(marriage-7, gay-6)
prep_for(struggle-4, marriage-7)
prepc_to(compared-2, conquering-9)
dobj(conquering-9, polio-10)
prepc_to(compared-2, getting-12)
conj_and(conquering-9, getting-12)
det(vote-14, the-13)
dobj(getting-12, vote-14)
appos(vote-14, etc.-16)

