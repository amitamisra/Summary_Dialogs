prep_to(extended-8, spouses-10)
nsubj(eligible-16, homosexuals-12)
aux(eligible-16, will-13)
neg(eligible-16, not-14)
cop(eligible-16, be-15)
ccomp(claims-2, eligible-16)
aux(receive-18, to-17)
