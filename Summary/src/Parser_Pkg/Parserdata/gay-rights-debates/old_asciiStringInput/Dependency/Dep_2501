nsubj(quotes-2, he-1)
dobj(quotes-2, him-3)
nsubj(says-5, S1-4)
dep(quotes-2, says-5)
nsubjpass(flattered-8, he-6)
auxpass(flattered-8, is-7)
ccomp(says-5, flattered-8)
mark(statement-18, that-9)
det(quote-11, a-10)
nsubj(statement-18, quote-11)
