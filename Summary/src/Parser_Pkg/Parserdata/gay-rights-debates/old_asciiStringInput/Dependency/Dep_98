auxpass(classified-5, is-4)
ccomp(feels-2, classified-5)
det(contract-9, a-7)
amod(contract-9, heterosexual-8)
agent(classified-5, contract-9)
cop(legal-13, is-11)
neg(legal-13, not-12)
ccomp(feels-2, legal-13)
conj_and(classified-5, legal-13)
conj_and(classified-5, natural-15)
conj_or(legal-13, natural-15)
mark(involves-18, if-16)
nsubj(involves-18, it-17)
ccomp(legal-13, involves-18)
dobj(involves-18, couples-19)
det(sex-23, the-21)
amod(sex-23, same-22)
prep_of(couples-19, sex-23)

