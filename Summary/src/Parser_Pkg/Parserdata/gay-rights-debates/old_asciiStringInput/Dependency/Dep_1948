det(politician-14, a-13)
agent(ignored-11, politician-14)
vmod(politician-14, attempting-15)
aux(make-17, to-16)
xcomp(attempting-15, make-17)
poss(law-21, her-18)
amod(law-21, religious-19)
nn(law-21, beliefs-20)
dobj(make-17, law-21)

