det(right-6, the-5)
dobj(have-4, right-6)
aux(marry-8, to-7)
vmod(right-6, marry-8)
dobj(marry-8, members-9)
det(sex-13, the-11)
amod(sex-13, opposite-12)
prep_of(members-9, sex-13)
det(sex-19, the-17)
amod(sex-19, same-18)
dobj(marry-8, sex-19)
