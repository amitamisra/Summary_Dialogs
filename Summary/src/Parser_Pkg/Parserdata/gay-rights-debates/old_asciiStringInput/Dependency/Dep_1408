amod(rights-8, gay-7)
prep_on(attacks-5, rights-8)
aux(cited-10, have-9)
ccomp(says-2, cited-10)
nn(Cameron-12, Paul-11)
dobj(cited-10, Cameron-12)
advmod(cited-10, even-14)
det(Senate-18, the-16)
nn(Senate-18, US-17)
prep_within(even-14, Senate-18)
vmod(cited-10, showing-20)
mark(has-23, that-21)
nsubj(has-23, he-22)
