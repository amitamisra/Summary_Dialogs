aux(force-6, to-5)
xcomp(attempting-4, force-6)
poss(morals-8, their-7)
dobj(force-6, morals-8)
prep_on(force-6, others-10)
prepc_by(force-6, legalizing-12)
amod(marriage-14, same-sex-13)
dobj(legalizing-12, marriage-14)
csubj(form-24, claiming-16)
mark(legalizing-18, that-17)
